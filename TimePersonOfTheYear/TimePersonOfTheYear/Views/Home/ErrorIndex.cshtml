@model List<TimePersonOfTheYear.Models.TimePerson>

<h1>Time People of the Year: 1927 - 2016</h1>
<p class="error">That range was not valid. Please try again.</p>
<p>Please select a range of years to view People Of The Year:</p>
<form method="post">

    <label name="startYear">Start Year</label>
    <input name="startYear" />

    <label name="endYear">End Year</label>
    <input name="endYear" />

    <button type="submit">Submit</button>

</form>